# Environment Configuration for Community Resource Navigation AI Agent

# App Settings
APP_NAME="Community Resource Navigation AI"
DEBUG=True
ENVIRONMENT=development

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@db:5432/community_resources

# OpenAI API (Required)
OPENAI_API_KEY=sk-your-api-key-here
OPENAI_MODEL=gpt-4-turbo-preview

# External Services API Keys (Optional but recommended)
GOOGLE_MAPS_API_KEY=your-google-maps-key-here
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE=+1234567890

# Cache Configuration
REDIS_URL=redis://redis:6379/0

# CORS Configuration
ALLOWED_ORIGINS=["http://localhost:3000","http://localhost:8000","http://localhost","http://127.0.0.1:3000"]

# Security
SECRET_KEY=your-secret-key-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=8000

# Logging
LOG_LEVEL=INFO
